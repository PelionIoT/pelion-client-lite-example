{
    "macros": [
        "MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config_cert.h\"",
        "MBED_CLOUD_CLIENT_USER_CONFIG_FILE=\"mbed_cloud_client_user_config_queue.h\""
    ],
    "requires": ["SecureStore",
                 "filesystemstore",
                 "filesystem",
                 "littlefs",
                 "storage_tdb_external",
                 "storage_tdb_external_no_rbp",
                 "storage_filesystem",
                 "storage_filesystem_no_rbp",
                 "qspif",
                 "flashiap-block-device",
                 "kv-config",
                 "kv-global-api",
                 "kv-map",
                 "tdbstore",
                 "storage",
                 "storage_tdb_internal",
                 "system-storage",
                 "bootloader",
                 "minimal-printf",
                 "mbed-client-randlib",
                 "device_key",
                 "mbed-protocol-manager",
                 "mbed-coap",
                 "nanostack-eventloop",
                 "bare-metal",
                 "rtos-api",
                 "mbed-cloud-client",
                 "mbed-client",
                 "update-client",
                 "nanostack-hal",
                 "client_app",
                 "cellular",
                 "nsapi",
                 "QUECTEL_BG96",
                 "ppp-cell-iface",
                 "nanostack-libservice",
                 "mbed-trace",
                 "events",
                 "softdevice",
                 "platform",
                 "mbedtls",
                 "mbed-crypto",
                 "psa",
                 "nordic", 
                 "delta-tool",
                 "esp8266",
                 "fat_chan"],
    "target_overrides": {
        "*": {
            "target.features_add"                       : ["BOOTLOADER"],
            "target.macros_add"                         : ["DISABLE_ERROR_DESCRIPTION=1"],
            "platform.stdio-baud-rate"                  : 115200,
            "platform.stdio-convert-newlines"           : true,
            "platform.stdio-buffered-serial"            : false,
            "platform.stdio-flush-at-exit"              : true,
            "platform.minimal-printf-enable-floating-point"             : false,
            "platform.minimal-printf-set-floating-point-max-decimals"   : 2,
            "platform.minimal-printf-enable-64-bit"                     : true,
            "nanostack-hal.event-loop-use-mbed-events"                  : true,
            "nanostack-hal.event-loop-dispatch-from-application"        : false,
            "nanostack-hal.critical-section-usable-from-interrupt"      : true,
            "events.shared-dispatch-from-application"   : true,
            "events.shared-eventsize"                   : 512,
            "events.shared-stacksize"                   : 1024,
            "events.shared-highprio-eventsize"          : 384,
            "mbed-trace.enable"                         : null,
            "mbed-client.enable-float-value"            : false,
            "mbed-client.event-loop-size"               : 512,
            "nsapi.dns-cache-size"                      : 0,
            "nsapi.default-cellular-apn"                : "\"internet\""
        },
        "NRF52840_DK": {
            "target.network-default-interface-type"     : "CELLULAR",
            "target.bootloader_img"                     : "tools/mbed-bootloader-nrf52840_dk-internal_flash-no_rot-v4.1.0-extras.hex",
            "target.OUTPUT_EXT"                         : "hex",
            "target.components_add"                     : ["QSPIF", "FLASHIAP"],
            "target.macros_remove"                      : ["MBEDTLS_CONFIG_HW_SUPPORT"],
            "target.extra_labels_add"                   : ["SOFTDEVICE_NONE"],
            "target.extra_labels_remove"                : ["NORDIC_CORDIO", "CORDIO", "CORDIO_LL", "SOFTDEVICE_COMMON", "SOFTDEVICE_S140_FULL", "NORDIC_SOFTDEVICE"],
            "target.features_remove"                    : ["CRYPTOCELL310"],
            "target.header_offset"                      : "0x8000",
            "target.app_offset"                         : "0x8400",
            "target.restrict_size"                      : "0x77C00",
            "mbed-client.sn-coap-max-blockwise-payload-size" : 256,
            "mbed-cloud-client.storage-type"            : "KVSTORE",
            "mbed-cloud-client-update-buffer-size"      : 1024,
            "mbed-cloud-client.update-storage"          : "ARM_UCP_FLASHIAP",
            "storage_tdb_internal.internal_base_address": "(512*1024)",
            "storage_tdb_internal.internal_size"        : "(32*1024)",
            "storage.storage_type"                      : "TDB_INTERNAL",
            "update-client.application-details"         : "(MBED_ROM_START + MBED_BOOTLOADER_SIZE)",
            "update-client.storage-address"             : "(MBED_CONF_STORAGE_TDB_INTERNAL_INTERNAL_BASE_ADDRESS+MBED_CONF_STORAGE_TDB_INTERNAL_INTERNAL_SIZE)",
            "update-client.storage-size"                : "(512*1024-MBED_CONF_STORAGE_TDB_INTERNAL_INTERNAL_SIZE)",
            "update-client.storage-locations"           : 1,
            "update-client.storage-page"                : 1,
            "QUECTEL_BG96.provide-default"              : true,
            "ppp-cell-iface.baud-rate"                  : 57600,
            "ppp-cell-iface.apn-lookup"                 : false,
            "cellular.debug-at"                         : true,
            "cellular.radio-access-technology"          : 3,
            "cellular.use-sms"                          : 0,
            "cellular.clear-on-connect"                 : true,
            "QUECTEL_BG96.tx"                           : "p42",
            "QUECTEL_BG96.rx"                           : "p40",
            "QUECTEL_BG96.cts"                          : "p35",
            "QUECTEL_BG96.rts"                          : "p36",
            "QUECTEL_BG96.pwr"                          : "P1_6",
            "QUECTEL_BG96.rst"                          : "P1_5"
        }
    },
    "config": {
        "developer-mode": {
            "help": "Enable Developer mode to skip Factory enrollment",
            "value": 1
        },
        "button-resource": {
            "help": "PinName for button.",
            "value": "BUTTON1"
        },
        "button-unregister": {
            "help": "PinName for button.",
            "value": "NC"
        },
        "led-pinname": {
            "help": "PinName for led, which is attached to led blink resource.",
            "value": "NC"
        },
        "bootloader-size": {
            "help"      : "Helper macro to enable calculation of rom regions. target.header_offset and target.app_offset still needs to be calculated manually, though.",
            "value"     : "(32*1024)",
            "macro_name": "MBED_BOOTLOADER_SIZE"
        },
        "mbed-cloud-client-update-buffer-size": {
            "value"     : null
        }

    }
}
